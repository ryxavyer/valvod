{
    "compilerOptions": {
      "paths": {
        "@src/*": [
          "./src/*"
        ],
        "@public/*": [
          "./public/*"
        ],
      },
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": false,
      "noEmit": true,
      "incremental": true,
      "module": "esnext",
      "target": "ES2020",
      "esModuleInterop": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "preserve",
      "plugins": [
        {
          "name": "next"
        }
      ]
    },
    "include": [
      "next-env.d.ts",
      ".next/types/**/*.ts",
      "**/*.ts",
      "**/*.tsx"
, "next.config.js", "postcss.config.js", "next.config.js", "next.config.js", "postcss.config.js"    ],
    "exclude": [
      "node_modules"
    ]
}
